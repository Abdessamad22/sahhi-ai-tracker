<!-- أيقونة خاصة بأجهزة Apple -->
<svg width="180" height="180" viewBox="0 0 180 180" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="appleGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0abe70;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00774b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Rounded rectangle background (Apple style) -->
  <rect x="10" y="10" width="160" height="160" rx="35" fill="url(#appleGradient)"/>
  
  <!-- Health cross symbol -->
  <rect x="72" y="52" width="36" height="76" rx="8" fill="white"/>
  <rect x="52" y="72" width="76" height="36" rx="8" fill="white"/>
  
  <!-- Heart symbol -->
  <path d="M90 115 L80 105 C75 100 75 92 80 87 C85 82 95 82 100 87 C105 92 105 100 100 105 L90 115 Z" fill="url(#appleGradient)"/>
  
  <!-- Text -->
  <text x="90" y="155" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="white">صحتي</text>
</svg>